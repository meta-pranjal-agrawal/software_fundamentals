import java.util.ArrayList;
import java.util.List;


public class Zoo 
{
	
	List<Zone> zoneList= new ArrayList<Zone>();
	
	public void addZone(String category,int limitOfCages,boolean hasPark,boolean hasCanteen)
	{
		Zone z1= new Zone(category,limitOfCages,hasPark,hasCanteen);
		zoneList.add(z1);
		
	}
	
	public boolean addCage(String category,int capacity,String type)
	{
		for(int i=0;i<zoneList.size();i++)
		{
			if(zoneList.get(i).category==category && zoneList.get(i).limitOfCages>zoneList.get(i).numberOfCages)
			{
				
			}
		}
		
		return true;
	}
	
	


}
