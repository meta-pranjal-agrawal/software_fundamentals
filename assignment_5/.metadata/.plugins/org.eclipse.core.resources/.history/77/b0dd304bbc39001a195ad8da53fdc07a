import java.util.Scanner;


public class LCMHCF 
{
	
	public int LCM(int n1,int n2)
	{
		int lcm=0;
		if(n1>=n2)
		{
			lcm=n1;
		}
		else
		{
			lcm=n2;
		}
		while(true)
		{
			if(lcm%n1==0 && lcm%n2==0)
			{
				return lcm;
			}
			lcm++;
		}
	}
	
	public int HCF(int n1,int n2)
	{
		int gcd=1;
		int small=0;
		if(n1<=n2)
		{
			small=n1;
		}
		else
		{
			small=n2;
		}
		for(int i=1;i<=small;i++)
		{
			if(n1%i==0 && n2%i==0)
			{
				gcd=i;
			}
		}
		
		return gcd;
	}

	public static void main(String[] args) 
	{
		LCMHCF lcmhcf=new LCMHCF();
		System.out.println("Enter 2 numbers to calculate lcm");
		Scanner sc= new Scanner(System.in);
		int n1=sc.nextInt();
		int n2=sc.nextInt();
		int lcm=lcmhcf.LCM(n1, n2);
		System.out.println("LCM="+lcm);

	}

}
